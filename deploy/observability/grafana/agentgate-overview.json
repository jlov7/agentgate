{
  "title": "AgentGate Overview",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Tool Calls (rate)",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (decision) (rate(agentgate_tool_calls_total[5m]))",
          "legendFormat": "{{decision}}"
        }
      ]
    },
    {
      "id": 2,
      "title": "Request P95 Latency (s)",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(agentgate_request_duration_seconds_bucket[5m])))"
        }
      ]
    },
    {
      "id": 5,
      "title": "Request P99 Latency (s)",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.99, sum by (le) (rate(agentgate_request_duration_seconds_bucket[5m])))"
        }
      ]
    },
    {
      "id": 3,
      "title": "Kill Switch Activations",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(agentgate_kill_switch_activations_total[5m]))"
        }
      ]
    },
    {
      "id": 4,
      "title": "Rate Limit Denials",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(agentgate_rate_limit_hits_total[5m]))"
        }
      ]
    }
  ]
}
